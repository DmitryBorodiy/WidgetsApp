<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="SystemAccentColor" />

    <Color x:Key="SystemAltHighColorDark">#080808</Color>
    <Color x:Key="SystemAltHighColorLight">#FAFAFA</Color>

    <SolidColorBrush x:Key="SystemAltHighBrushDark" Color="{StaticResource SystemAltHighColorDark}" />
    <SolidColorBrush x:Key="SystemAltHighBrushLight" Color="{StaticResource SystemAltHighColorLight}" />

</ResourceDictionary>